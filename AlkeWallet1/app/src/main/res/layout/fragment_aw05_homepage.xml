<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.aw05_homepage"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/frg_header"
        android:layout_width="match_parent"
        android:layout_height="490dp"
        android:layout_weight="0.6"
        tools:layout="@layout/fragment_aw0x_header" >
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frg_buttons"
        android:layout_width="match_parent"
        android:layout_height="490dp"
        android:layout_weight="1"
        tools:layout="@layout/fragment_aw0x_buttons" >
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frg_transactions"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_aw0x_transactions"
        />

</LinearLayout>